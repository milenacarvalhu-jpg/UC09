    public static void main(String[] args) {

        int numero;

        Scanner in = new Scanner(System.in);
        System.out.println("Digite sua idade");
        numero = in.nextInt();

        if (numero < 18) {
            System.out.println("ele Ã© menor de idade");
        } else {
            System.out.println(" maior de idade");
        }
        in.close();

    }
}
